{
  "name": "closet-react",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "node scripts/dev.js",
    "dev:vite": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "start": "node server/api.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:extension": "jest extension-tests --verbose",
    "test:extension-cypress": "npx cypress run --spec 'cypress/e2e/extension-cookies.cy.js' --record --key 79b6b242-7610-47d0-b1a6-430738a87697",
    "test:extension-cypress-open": "npx cypress open --e2e",
    "test:cypress:ui": "cypress run --spec 'cypress/e2e/marketplace-ui.cy.js'",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "cypress": "cypress open",
    "cypress:run": "cypress run",
    "cypress:record": "cypress run --record --key 79b6b242-7610-47d0-b1a6-430738a87697",
    "test:api": "cypress run --spec 'cypress/e2e/marketplace-api.cy.js'",
    "prepare": "husky install",
    "migrate:items": "node --loader tsx scripts/run-migration.ts"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@pinecone-database/pinecone": "^2.2.1",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@shopify/shopify-api": "^12.1.2",
    "@shopify/shopify-app-remix": "^4.0.6",
    "@sparticuz/chromium": "^141.0.0",
    "@supabase/supabase-js": "^2.75.0",
    "@use-gesture/react": "^10.3.1",
    "@vercel/analytics": "^1.5.0",
    "@zxing/library": "^0.21.3",
    "axios": "^1.13.2",
    "chart.js": "^4.4.1",
    "cheerio": "^1.1.2",
    "clsx": "^2.1.0",
    "cmdk": "^0.2.1",
    "cors": "^2.8.5",
    "date-fns": "^3.3.1",
    "express": "^4.21.2",
    "framer-motion": "^12.23.22",
    "idb": "^8.0.0",
    "immer": "^10.0.3",
    "lucide-react": "^0.344.0",
    "postgres": "^3.4.7",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.3.1",
    "react-icons": "^5.5.0",
    "react-router-dom": "^6.22.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^2.2.1",
    "vaul": "^1.1.2",
    "zod": "^3.22.4",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.41.2",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^24.10.1",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vercel/node": "^5.5.6",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "@vitest/ui": "^1.2.2",
    "autoprefixer": "^10.4.17",
    "cypress": "^15.7.0",
    "dotenv": "^17.2.3",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "husky": "^9.0.10",
    "jest": "^30.2.0",
    "lint-staged": "^15.2.2",
    "openai": "^6.5.0",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "puppeteer": "^24.31.0",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.20.6",
    "typescript": "^5.3.3",
    "vite": "^5.1.0",
    "vite-plugin-pwa": "^0.17.5",
    "vitest": "^1.2.2",
    "workbox-window": "^7.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
